<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="config.mybatis.mapper.oracle.admin_board">
	<select id="selectUNotice" resultType="UserNoticeVO">
		select * from usernotice order by unotice_no desc
	</select>
	
	<select id="selectCNotice" resultType="CompanyNoticeVO">
		select * from companynotice order by cnotice_no desc
	</select>
	
	<select id="selectSCNotice" resultType="ServiceCenterNoticeVO">
		select * from servicecenternotice order by scnotice_no desc
	</select>
</mapper>









